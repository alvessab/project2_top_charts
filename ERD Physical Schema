

Artist
-
artist_ID INT PK
artist_name VARCHAR(500)
lastUpdate datetime

Provider
-
provider_ID INT PK
provider_name VARCHAR(50)
lastUpdate datetime

Album
-
album_ID INT PK
artist_ID INT FK >- Artist.artist_ID
album_title VARCHAR(500)
lastUpdate datetime


Song
-
song_ID INT PK
artist_ID INT FK >- Artist.artist_ID
album_ID INT FK >- Album.album_ID
song_name  VARCHAR(500)
song_length VARCHAR(10)
lastUpdate datetime

Rank_Junction
-
rank_ID INT
song_ID INT FK >- Song.song_ID
provider_ID FK >- Provider.provider_ID
rank INT
date_retrieved datetime
lastUpdate datetime



Spotify_Info
-
ID INT PK
song_ID INT FK >- Song.song_ID
stream_count INT
spotify_peak_rank INT
spotify_prev_rank INT
spotify_streak_days INT
lastUpdate datetime